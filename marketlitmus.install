<?php

/**
 * Implements hook_install().
 */
function marketlitmus_install() {
  theme_enable(array('marketlitmus'));
  variable_set('theme_default', 'marketlitmus');
}

function marketlitmus_user_default_roles() {
  $roles = array();

  // Exported role: Manager
  $roles['manager'] = array(
    'name' => 'manager',
    'weight' => '2',
  );
  return $roles;
}